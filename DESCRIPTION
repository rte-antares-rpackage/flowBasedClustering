Package: flowBasedClustering
Type: Package
Title: Cluster 'FlowBased' Typical Days 
Version: 1.0.0
Authors@R: c(
    person("Baptiste", "Seguinot", role = c("aut", "cre"), email = "baptiste.seguinot@rte-france.com"),
    person("Marion", "Li", role = c("aut"), email = "marion.li@rte-france.com"),
